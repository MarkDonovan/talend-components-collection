<?xml version="1.0" encoding="UTF-8"?>
<COMPONENT>
	<HEADER AUTHOR="$COMPONENT-AUTHOR$" COMPATIBILITY="ALL"
		PLATEFORM="ALL" RELEASE_DATE="$RELEASE-DATE$" SERIAL=""
		SHORTNAME="$COMPONENT-NAME$" STARTABLE="true" STATUS="RELEASED"
		VERSION="$COMPONENT-VERSION$" PARTITIONING="AUTO">
		<SIGNATURE />
	</HEADER>
	<FAMILIES>
		<FAMILY>Custom_Code</FAMILY>
	</FAMILIES>
	<DOCUMENTATION>
		<URL>http://gabrielebaldassarre.com/talend/texecuterscript</URL>
	</DOCUMENTATION>
	<CONNECTORS>
		<CONNECTOR CTYPE="FLOW" MAX_INPUT="0" />
		<CONNECTOR CTYPE="ITERATE" />
		<CONNECTOR CTYPE="SUBJOB_OK" />
		<CONNECTOR CTYPE="SUBJOB_ERROR" />
		<CONNECTOR CTYPE="COMPONENT_OK" />
		<CONNECTOR CTYPE="COMPONENT_ERROR" />
		<CONNECTOR CTYPE="RUN_IF" />
	</CONNECTORS>
	<PARAMETERS>
		<PARAMETER NAME="R_SCRIPT" FIELD="CHECK" REQUIRED="true"
			NUM_ROW="10">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="R_CODE" FIELD="MEMO" REQUIRED="true"
			NUM_ROW="11" NB_LINES="5" NOT_SHOW_IF="(R_SCRIPT=='true')">
			<DEFAULT>"print(head(iris))"</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="R_FILE" FIELD="FILE" NUM_ROW="12" REQUIRED="true"
			NOT_SHOW_IF="(R_SCRIPT=='false')">
			<DEFAULT>"__COMP_DEFAULT_FILE_DIR__/iris.R"</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="R_START_ARGS" FIELD="TEXT" REQUIRED="true"
			NUM_ROW="20">
			<DEFAULT>"--no-save"</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="SEND_TO_LOGCATCHER" FIELD="CHECK" REQUIRED="true"
			NUM_ROW="30">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>
	</PARAMETERS>
	<ADVANCED_PARAMETERS>
		<PARAMETER NAME="SCHEMA" FIELD="SCHEMA_TYPE" REQUIRED="false"
			NUM_ROW="10" SHOW="true" GROUP="R_OUTPUT">
			<TABLE />
		</PARAMETER>
		<PARAMETER FIELD="TEXT" GROUP="R_OUTPUT" NAME="LOOP_SYMBOL"
			NUM_ROW="11" REQUIRED="false" />
		<PARAMETER NAME="MAPPING" FIELD="TABLE" GROUP="R_OUTPUT"
			REQUIRED="false" NUM_ROW="12" NB_LINES="5">
			<ITEMS BASED_ON_SCHEMA="true">
				<ITEM NAME="R_SYMBOL" FIELD="String" />
				<ITEM NAME="REXP" FIELD="CLOSED_LIST">
					<ITEMS DEFAULT="INT">
						<ITEM NAME="DOUBLE" VALUE="DOUBLE" />
						<ITEM NAME="INT" VALUE="INT" />
						<ITEM NAME="STRING" VALUE="STRING" />
					</ITEMS>
				</ITEM>
			</ITEMS>
		</PARAMETER>
		<PARAMETER NAME="CLIENT_TYPE" FIELD="CLOSED_LIST" NUM_ROW="20"
			REQUIRED="true">
			<ITEMS DEFAULT="STANDARD">
				<ITEM NAME="STANDARD" VALUE="STANDARD" />
				<ITEM NAME="SILENT" VALUE="SILENT" />
			</ITEMS>
		</PARAMETER>
	</ADVANCED_PARAMETERS>
	<CODEGENERATION>
		<!-- Maven-managed import list DO NOT EDIT BELOW THIS LINE -->
		<!-- TOSIMPORTS -->
		<!-- You can edit again below this line -->
	</CODEGENERATION>
	<RETURNS>
	</RETURNS>
</COMPONENT>
