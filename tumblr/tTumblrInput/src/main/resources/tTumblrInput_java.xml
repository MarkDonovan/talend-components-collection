<?xml version="1.0" encoding="UTF-8"?>
<COMPONENT>
	<HEADER AUTHOR="$COMPONENT-AUTHOR$" COMPATIBILITY="ALL"
		DATA_AUTO_PROPAGATE="true" PLATEFORM="ALL" RELEASE_DATE="$RELEASE-DATE$"
		SCHEMA_AUTO_PROPAGATE="true" SERIAL="" SHORTNAME="$COMPONENT-NAME$"
		STARTABLE="true" STATUS="BETA" VERSION="$COMPONENT-VERSION$">
		<SIGNATURE />
	</HEADER>
	<FAMILIES>
		<FAMILY>Social Analytics/Tumblr</FAMILY>
	</FAMILIES>
	<DOCUMENTATION>
		<URL>http://gabrielebaldassarre.com/tumblr/tumblrinput</URL>
	</DOCUMENTATION>
	<CONNECTORS>
		<CONNECTOR CTYPE="FLOW" MAX_INPUT="0" MAX_OUTPUT="0" />
		<CONNECTOR NAME="JSON" CTYPE="FLOW" MAX_INPUT="0" MAX_OUTPUT="0" COLOR="f36300" />
		<CONNECTOR NAME="STANDARD" CTYPE="FLOW" MAX_INPUT="0" />
		<CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="0" MAX_INPUT="1" />
		<CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1" />
		<CONNECTOR CTYPE="COMPONENT_OK" />
		<CONNECTOR CTYPE="COMPONENT_ERROR" />
		<CONNECTOR CTYPE="RUN_IF" />
	</CONNECTORS>
	<PARAMETERS>
		<PARAMETER NAME="SCHEMA" FIELD="SCHEMA_TYPE" REQUIRED="false"
			NUM_ROW="1" SHOW="true" CONTEXT="STANDARD">
			<TABLE>
				<COLUMN NAME="postId" TYPE="id_Long" LENGTH="22" />
			</TABLE>
		</PARAMETER>
		<PARAMETER NAME="OAUTH_CONNECTION" FIELD="COMPONENT_LIST"
			REQUIRED="true" FILTER="tTumblrOAuth" NUM_ROW="1" />
		<PARAMETER NAME="TAG" FIELD="TEXT" REQUIRED="false" NUM_ROW="10" />
		<PARAMETER NAME="BEFORE" FIELD="TEXT" REQUIRED="false"
			NUM_ROW="10" />
		<PARAMETER NAME="POST_TYPE" FIELD="CLOSED_LIST" NUM_ROW="20"
			REQUIRED="true">
			<ITEMS DEFAULT="GENERIC">
				<ITEM NAME="GENERIC" VALUE="GENERIC" />
				<ITEM NAME="TEXT" VALUE="TEXT" />
				<ITEM NAME="PHOTO" VALUE="PHOTO" />
				<ITEM NAME="QUOTE" VALUE="QUOTE" />
				<ITEM NAME="LINK" VALUE="LINK" />
				<ITEM NAME="CHAT" VALUE="CHAT" />
				<ITEM NAME="AUDIO" VALUE="AUDIO" />
				<ITEM NAME="VIDEO" VALUE="VIDEO" />
				<ITEM NAME="ANSWER" VALUE="ANSWER" />
			</ITEMS>
		</PARAMETER>

		<PARAMETER NAME="MAPPING" FIELD="TABLE" REQUIRED="true"
			NUM_ROW="30" NB_LINES="5">
			<ITEMS BASED_ON_SCHEMA="true">
				<ITEM NAME="CATEGORY" FIELD="CLOSED_LIST">
					<ITEMS DEFAULT="GENERIC">
						<ITEM NAME="GENERIC" VALUE="GENERIC" />
						<ITEM NAME="TEXT" VALUE="TEXT" SHOW_IF="(POST_TYPE == 'TEXT') or (POST_TYPE == 'GENERIC')" />
						<ITEM NAME="PHOTO" VALUE="PHOTO" SHOW_IF="(POST_TYPE == 'PHOTO') or (POST_TYPE == 'GENERIC')" />
						<ITEM NAME="QUOTE" VALUE="QUOTE" SHOW_IF="(POST_TYPE == 'QUOTE') or (POST_TYPE == 'GENERIC')" />
						<ITEM NAME="LINK" VALUE="LINK" SHOW_IF="(POST_TYPE == 'LINK') or (POST_TYPE == 'GENERIC')" />
						<ITEM NAME="CHAT" VALUE="CHAT" SHOW_IF="(POST_TYPE == 'CHAT') or (POST_TYPE == 'GENERIC')" />
						<ITEM NAME="AUDIO" VALUE="AUDIO" SHOW_IF="(POST_TYPE == 'AUDIO') or (POST_TYPE == 'GENERIC')" />
						<ITEM NAME="VIDEO" VALUE="VIDEO" SHOW_IF="(POST_TYPE == 'VIDEO') or (POST_TYPE == 'GENERIC')" />
						<ITEM NAME="ANSWER" VALUE="ANSWER" SHOW_IF="(POST_TYPE == 'ANSWER') or (POST_TYPE == 'GENERIC')" />
					</ITEMS>
				</ITEM>
				<ITEM NAME="OPERATION" FIELD="CLOSED_LIST">
					<ITEMS DEFAULT="POST_ID">
						<ITEM NAME="POST_ID" VALUE="POST_ID" SHOW_IF="(MAPPING.CATEGORY == 'GENERIC')" />
						<ITEM NAME="BLOG_NAME" VALUE="BLOG_NAME" SHOW_IF="(MAPPING.CATEGORY == 'GENERIC')" />
						<ITEM NAME="DATE_GMT" VALUE="DATE_GMT" SHOW_IF="(MAPPING.CATEGORY == 'GENERIC')" />
						<ITEM NAME="FORMAT" VALUE="FORMAT" SHOW_IF="(MAPPING.CATEGORY == 'GENERIC')" />
						<ITEM NAME="NOTE_COUNT" VALUE="NOTE_COUNT" SHOW_IF="(MAPPING.CATEGORY == 'GENERIC')" />
						<ITEM NAME="POST_URL" VALUE="POST_URL" SHOW_IF="(MAPPING.CATEGORY == 'GENERIC')" />
						<ITEM NAME="REBLOGGED_FROM_ID" VALUE="REBLOGGED_FROM_ID" SHOW_IF="(MAPPING.CATEGORY == 'GENERIC')" />
						<ITEM NAME="REBLOGGED_FROM_NAME" VALUE="REBLOGGED_FROM_NAME" SHOW_IF="(MAPPING.CATEGORY == 'GENERIC')" />
						<ITEM NAME="REBLOG_KEY" VALUE="REBLOG_KEY" SHOW_IF="(MAPPING.CATEGORY == 'GENERIC')" />
						<ITEM NAME="SLUG" VALUE="SLUG" SHOW_IF="(MAPPING.CATEGORY == 'GENERIC')" />
						<ITEM NAME="SOURCE_TITLE" VALUE="SOURCE_TITLE" SHOW_IF="(MAPPING.CATEGORY == 'GENERIC')" />
						<ITEM NAME="SOURCE_URL" VALUE="SOURCE_URL" SHOW_IF="(MAPPING.CATEGORY == 'GENERIC')" />
						<ITEM NAME="STATE" VALUE="STATE" SHOW_IF="(MAPPING.CATEGORY == 'GENERIC')" />
						<ITEM NAME="TAGS" VALUE="TAGS" SHOW_IF="(MAPPING.CATEGORY == 'GENERIC')" />
						<ITEM NAME="TIMESTAMP" VALUE="TIMESTAMP" SHOW_IF="(MAPPING.CATEGORY == 'GENERIC')" />
						<ITEM NAME="TYPE" VALUE="TYPE" SHOW_IF="(MAPPING.CATEGORY == 'GENERIC')" />
						<ITEM NAME="IS_BOOKMARKLET" VALUE="IS_BOOKMARKLET" SHOW_IF="(MAPPING.CATEGORY == 'GENERIC')" />
						<ITEM NAME="IS_LINKED" VALUE="IS_LINKED" SHOW_IF="(MAPPING.CATEGORY == 'GENERIC')" />
						<ITEM NAME="IS_MOBILE" VALUE="IS_MOBILE" SHOW_IF="(MAPPING.CATEGORY == 'GENERIC')" />
						<ITEM NAME="PHOTO_HEIGHT" VALUE="PHOTO_HEIGHT" SHOW_IF="(MAPPING.CATEGORY == 'PHOTO')" />
						<ITEM NAME="PHOTO_URL" VALUE="PHOTO_URL" SHOW_IF="(MAPPING.CATEGORY == 'PHOTO')" />
						<ITEM NAME="PHOTO_WIDTH" VALUE="PHOTO_WIDHT" SHOW_IF="(MAPPING.CATEGORY == 'PHOTO')" />
						<ITEM NAME="TITLE" VALUE="TITLE" SHOW_IF="(MAPPING.CATEGORY == 'TEXT') or (MAPPING.CATEGORY == 'CHAT') or (MAPPING.CATEGORY == 'LINK')"/>
						<ITEM NAME="BODY" VALUE="BODY" SHOW_IF="(MAPPING.CATEGORY == 'TEXT') or (MAPPING.CATEGORY == 'CHAT')" />
						<ITEM NAME="DIALOGUE" VALUE="DIALOGUE" SHOW_IF="(MAPPING.CATEGORY == 'CHAT')" />
						<ITEM NAME="TEXT" VALUE="TEXT" SHOW_IF="(MAPPING.CATEGORY == 'QUOTE')" />
						<ITEM NAME="SOURCE" VALUE="SOURCE" SHOW_IF="(MAPPING.CATEGORY == 'QUOTE')" />
						<ITEM NAME="DESCRIPTION" VALUE="DESCRIPTION" SHOW_IF="(MAPPING.CATEGORY == 'LINK')" />
						<ITEM NAME="LINK_URL" VALUE="LINK_URL" SHOW_IF="(MAPPING.CATEGORY == 'LINK')" />
						<ITEM NAME="ALBUM_ART_URL" VALUE="ALBUM_ART_URL" SHOW_IF="(MAPPING.CATEGORY == 'AUDIO')" />
						<ITEM NAME="ALBUM_NAME" VALUE="ALBUM_NAME" SHOW_IF="(MAPPING.CATEGORY == 'AUDIO')" />
						<ITEM NAME="ARTIST_NAME" VALUE="ARTIST_NAME" SHOW_IF="(MAPPING.CATEGORY == 'AUDIO')" />
						<ITEM NAME="CAPTION" VALUE="CAPTION" SHOW_IF="(MAPPING.CATEGORY == 'PHOTO') or (MAPPING.CATEGORY == 'AUDIO') or (MAPPING.CATEGORY == 'VIDEO')"/>
						<ITEM NAME="EMBED_CODE" VALUE="EMBED_CODE" SHOW_IF="(MAPPING.CATEGORY == 'AUDIO') or (MAPPING.CATEGORY == 'VIDEO')" />
						<ITEM NAME="PLAY_COUNT" VALUE="PLAY_COUNT" SHOW_IF="(MAPPING.CATEGORY == 'AUDIO')" />
						<ITEM NAME="TRACK_NAME" VALUE="TRACK_NAME" SHOW_IF="(MAPPING.CATEGORY == 'AUDIO')" />
						<ITEM NAME="TRACK_NUMBER" VALUE="TRACK_NUMBER" SHOW_IF="(MAPPING.CATEGORY == 'AUDIO')" />
