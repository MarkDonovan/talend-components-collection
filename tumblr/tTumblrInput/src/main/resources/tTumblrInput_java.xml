						<ITEM NAME="THUMBNAIL_WIDHT" VALUE="THUMBNAIL_WIDTH" SHOW_IF="(MAPPING.CATEGORY == 'VIDEO')" />
						<ITEM NAME="THUMBNAIL_HEIGHT" VALUE="THUMBNAIL_HEIGHT" SHOW_IF="(MAPPING.CATEGORY == 'VIDEO')" />
