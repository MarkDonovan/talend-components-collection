				<ITEM NAME="TEXT" VALUE="TEXT" />
				<ITEM NAME="RAW" VALUE="RAW" />
			</ITEMS>
		</PARAMETER>
		<PARAMETER NAME="SEND_TO_LOGCATCHER" FIELD="CHECK" REQUIRED="true"
			NUM_ROW="50">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>
	</PARAMETERS>
	<ADVANCED_PARAMETERS>
		<PARAMETER NAME="LIMIT" FIELD="TEXT" REQUIRED="true"
			NUM_ROW="20">
			<DEFAULT>20</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="ENTITIES_SEPARATOR" FIELD="TEXT" REQUIRED="true"
			NUM_ROW="30">
			<DEFAULT>","</DEFAULT>
		</PARAMETER>
	</ADVANCED_PARAMETERS>
