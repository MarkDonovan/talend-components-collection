		SCHEMA_AUTO_PROPAGATE="true" SERIAL="" SHORTNAME="$COMPONENT-NAME$"
		STARTABLE="true" STATUS="BETA" VERSION="$COMPONENT-VERSION$">
		<SIGNATURE />
	</HEADER>
	<FAMILIES>
		<FAMILY>Atlassian JIRA</FAMILY>
	</FAMILIES>
	<DOCUMENTATION>
		<URL>http://gabrielebaldassarre.com</URL>
	</DOCUMENTATION>
	<CONNECTORS>
		<CONNECTOR CTYPE="FLOW" MAX_INPUT="0" />
		<CONNECTOR CTYPE="ITERATE" MAX_INPUT="1" />
		<CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1" />
		<CONNECTOR CTYPE="SUBJOB_ERROR" MAX_INPUT="1" />
		<CONNECTOR CTYPE="COMPONENT_OK" />
		<CONNECTOR CTYPE="COMPONENT_ERROR" />
		<CONNECTOR CTYPE="RUN_IF" />
	</CONNECTORS>
	<PARAMETERS>
		<PARAMETER NAME="SCHEMA" FIELD="SCHEMA_TYPE" REQUIRED="false"
			NUM_ROW="1" SHOW="true">
			<TABLE>
          <COLUMN NAME="projectKey" TYPE="id_String" LENGTH="128"/>
      </TABLE>
		</PARAMETER>
		<PARAMETER NAME="JIRA_HOST" FIELD="TEXT" NUM_ROW="2" REQUIRED="true">
			<DEFAULT>"https://localhost.localdomain:9001"</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="JIRA_USER" FIELD="TEXT" NUM_ROW="3" REQUIRED="true">
			<DEFAULT>"jirauser"</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="JIRA_PASS" FIELD="TEXT" NUM_ROW="3" REQUIRED="true">
			<DEFAULT>"jirapassword"</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="CONDITIONS" FIELD="TABLE" REQUIRED="true" NUM_ROW="20" NB_LINES="5">
			<ITEMS BASED_ON_SCHEMA="true">
				<ITEM NAME="OPERATION" FIELD="CLOSED_LIST">
					<ITEMS DEFAULT="KEY">
						<ITEM NAME="KEY" VALUE="KEY" />
						<ITEM NAME="SELF_STRING" VALUE="SELF_STRING" />
						<ITEM NAME="DESCRIPTION" VALUE="DESCRIPTION" />
						<ITEM NAME="URI_STRING" VALUE="URI_STRING" />
						<ITEM NAME="LEAD_NAME" VALUE="LEAD_NAME" />
						<ITEM NAME="LEAD_DISPLAYNAME" VALUE="LEAD_DISPLAYNAME" />
						<ITEM NAME="LEAD_SELF_STRING" VALUE="LEAD_SELF_STRING" />
						<ITEM NAME="COMPONENTS_NAME" VALUE="COMPONENTS_NAME" />
						<ITEM NAME="COMPONENTS_SELF_STRING" VALUE="COMPONENTS_SELF_STRING" />
						<ITEM NAME="VERSIONS_DESCRIPTION" VALUE="VERSIONS_DESCRIPTION" />
						<ITEM NAME="VERSIONS_NAME" VALUE="VERSIONS_NAME" />
						<ITEM NAME="VERSIONS_RELEASE_DATE" VALUE="VERSIONS_RELEASE_DATE" />
						<ITEM NAME="VERSIONS_SELF_STRING" VALUE="VERSIONS_SELF_STRING" />
						<ITEM NAME="VERSIONS_RELEASE_DATE" VALUE="VERSIONS_RELEASE_DATE" />
						<ITEM NAME="VERSIONS_IS_ARCHIVED" VALUE="VERSIONS_IS_ARCHIVED" />
						<ITEM NAME="VERSIONS_IS_RELEASED" VALUE="VERSIONS_IS_RELEASED" />
					</ITEMS>
				</ITEM>
			</ITEMS>
		</PARAMETER>
	</PARAMETERS>
	<ADVANCED_PARAMETERS>
			<PARAMETER NAME="EXCLUSION_MODE" FIELD="CHECK" NUM_ROW="10"
			REQUIRED="true" >
				<DEFAULT>true</DEFAULT>
			</PARAMETER>
		<PARAMETER NAME="PROJECT_LIST" FIELD="TABLE" REQUIRED="false"
			NUM_ROW="11" NB_LINES="5">
			<ITEMS>
				<ITEM NAME="PROJECT" FIELD="String" />
			</ITEMS>
		</PARAMETER>
	</ADVANCED_PARAMETERS>
	<CODEGENERATION>
		<!-- Maven-managed import list DO NOT EDIT BELOW THIS LINE -->
		<!-- TOSIMPORTS -->
		<!-- You can edit again below this line -->
	</CODEGENERATION>
	<RETURNS>
		<RETURN AVAILABILITY="AFTER" NAME="NB_LINE" TYPE="id_Integer" />
		<RETURN AVAILABILITY="FLOW" NAME="CURRENT_PROJECT_FLOW" TYPE="id_String" />
		<RETURN AVAILABILITY="FLOW" NAME="CURRENT_PROJECT_SELF" TYPE="id_String" />
	</RETURNS>
</COMPONENT>
